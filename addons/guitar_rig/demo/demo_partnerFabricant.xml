            <field name="image_1920" type="base64" file="guitar_rig/static/img/boss_logo.png"/>
        </record>

        <record id="xml_partner_gibson" model="res.partner">
                    <field name="name">Gibson Brands</field>
                    <field name="is_company">True</field>
                    <field name="company_type">company</field>
                    <field name="is_guitar_brand">True</field>
                    <field name="brand_type">industrial</field>
                    <field name="country_id" ref="base.us"/>
                    <field name="history">Fondée en 1902 par Orville Gibson à Kalamazoo, Michigan. 
Gibson est célèbre pour ses guitares acoustiques et électriques emblématiques comme la Les Paul et la SG. 
La marque a joué un rôle crucial dans le développement du son rock et blues.</field>
                    <field name="image_1920" type="base64" file="guitar_rig/static/img/gibson_logo.png"/>

        </record>

                <record id="xml_partner_ibanez" model="res.partner">
                    <field name="name">Ibanez Guitars</field>
                    <field name="is_company">True</field>
                    <field name="company_type">company</field>
                    <field name="is_guitar_brand">True</field>
                    <field name="brand_type">industrial</field>
                    <field name="country_id" ref="base.jp"/>
                    <field name="history">Marque japonaise fondée en 1957, célèbre pour ses guitares électriques innovantes et abordables. 
Ibanez a gagné en popularité dans les années 1980 grâce à ses modèles adaptés au heavy metal et au shred guitar.</field>
                    <field name="image_1920" type="base64" file="guitar_rig/static/img/ibanez_logo.png"/>
                </record>

<record id="xml_partner_marshall" model="res.partner">
            <field name="name">Marshall Amplification</field>
            <field name="is_company">True</field>
            <field name="company_type">company</field>
            <field name="is_guitar_brand">True</field>
            <field name="brand_type">industrial</field>
            <field name="country_id" ref="base.uk"/> 
            <field name="image_1920" type="base64" file="guitar_rig/static/img/marshall_logo.jpg"/>
            <field name="history">Fondée en 1962 par Jim Marshall à Londres, Marshall est devenue une légende dans le monde des amplificateurs de guitare. 
Les amplis Marshall, avec leur son distinctif "crunchy", ont été adoptés par des icônes du rock comme Jimi Hendrix, Eric Clapton et Slash.</field>
            </record>

        <record id="xml_partner_vox" model="res.partner">
            <field name="name">Vox Amplification</field>
            <field name="is_company">True</field>
            <field name="company_type">company</field>
            <field name="is_guitar_brand">True</field>
            <field name="brand_type">industrial</field>
            <field name="country_id" ref="base.uk"/>
            <field name="image_1920" type="base64" file="guitar_rig/static/img/vox_logo.png"/>
            <field name="history">Fondée en 1957 au Royaume-Uni, Vox est célèbre pour ses amplificateurs emblématiques comme le AC30, utilisé par des groupes légendaires tels que The Beatles et The Rolling Stones.</field>
        </record>

        <record id="xml_partner_gretsch" model="res.partner">
            <field name="name">Gretsch Guitars</field>
            <field name="is_company">True</field>
            <field name="company_type">company</field>
            <field name="is_guitar_brand">True</field>
            <field name="brand_type">industrial</field>
            <field name="country_id" ref="base.us"/>
            <field name="history">Fondée en 1883 par Friedrich Gretsch à Brooklyn, New York. 
Gretsch est renommée pour ses guitares électriques et acoustiques de haute qualité,
notamment les modèles à caisse creuse utilisés dans le rockabilly et le jazz.</field>
            <field name="image_1920" type="base64" file="guitar_rig/static/img/gretsch.png"/>
