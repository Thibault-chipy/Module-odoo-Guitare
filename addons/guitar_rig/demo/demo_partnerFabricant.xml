        <record id="xml_partner_fender" model="res.partner">
            <field name="name">Fender Musical Instruments Corp.</field>
            <field name="is_company">True</field>
            <field name="company_type">company</field>
            
            <field name="street">17600 N. Perimeter Drive, Suite 100</field>
            <field name="city">Scottsdale</field>
            <field name="zip">85255</field>
            
            <field name="country_id" ref="base.us"/>
            
            <field name="phone">+1 480-596-9600</field>
            <field name="email">contact@fender.com</field>
            <field name="website">https://www.fender.com</field>

            <field name="is_guitar_brand">True</field> 
            <field name="brand_type">industrial</field>
            
            <field name="history">Fondée en 1946 par Leo Fender à Fullerton, Californie. 
Fender est devenue une icône culturelle avec l'invention de la Telecaster (1950) et de la Stratocaster (1954). 
La marque a révolutionné la musique moderne en démocratisant la guitare électrique Solid Body.</field>

            <field name="image_1920" type="base64" file="guitar_rig/static/img/fender_logo.jpg"/>

            <field name="category_id" eval="[(6,0,[])]"/>
        </record>

